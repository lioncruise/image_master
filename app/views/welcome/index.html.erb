<head>
  <!-- Meta, title, CSS, favicons, etc. -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">


  <title>

    ImageMaster

  </title>
  <!-- Bootstrap core CSS -->

</head>
<body class="bs-docs-home">
<a class="sr-only sr-only-focusable" href="#content">Skip to main content</a>

<!-- Docs master nav -->
<header class="navbar navbar-static-top bs-docs-nav" id="top" role="banner">
  <div class="container">
    <div class="navbar-header">
      <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a href="../" class="navbar-brand">ImageMaster</a>
    </div>
    <nav class="collapse navbar-collapse bs-navbar-collapse" role="navigation">
      <ul class="nav navbar-nav">
        <li>
          <a href="../getting-started/">Account</a>
        </li>
        <li>
          <a href="../css/">Myphotos</a>
        </li>
      </ul>
      <ul class="nav navbar-nav navbar-right">

        <li><a href="sessions/new" onclick="_hmt.push(['_trackEvent', 'docv3-navbar', 'click', 'doc-home-navbar-job'])" target="_blank">Sign in</a></li>
        <li><a href="applicants/new" onclick="_hmt.push(['_trackEvent', 'docv3-navbar', 'click', 'doc-home-navbar-expo'])" target="_blank">Sign up</a></li>
        <li><a href="http://blog.getbootstrap.com" onclick="_hmt.push(['_trackEvent', 'docv3-navbar', 'click', 'doc-home-navbar-blog'])" target="_blank">About us</a></li>
      </ul>
    </nav>
  </div>
</header>


<!-- Page content of course! -->
<main class="bs-docs-masthead" id="content" role="main">

</main>
<div class="bs-docs-featurette">
  <div class="container">
    <label class="col-sm-2"></label>
    <div class="col-sm-8" height="100px">
      <%= image_tag('logo.bmp', :class=>'img-responsive') %>
    </div>
    <label class="col-sm-2"></label>
  </div>
  <div class="container">
    <%= form_for :keyword, :url => welcome_show_path do |f| %>
        <div class="form-horizontal">
          <div class="form-group">
            <label  class="col-sm-2 control-label"></label>
            <div class="col-sm-6"><%= f.text_field :name ,:class=>'form-control' %></div>
            <div class="col-sm-2"><%= f.submit :search, :class=>'btn btn-primary form-control' %></div>
          </div>
        </div>
    <% end %>
  </div>
</div>



<!-- Footer
================================================== -->
<footer class="bs-docs-footer" role="contentinfo">
  <div class="container">

  </div>
</footer>
</body>