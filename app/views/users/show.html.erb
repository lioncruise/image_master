<div class="container">

    <div class="container projects">

        <div class="row">
            <%= @pictures.each do |picture| %>
            <div class="col-sm-6 col-md-4 col-lg-3 ">
                <div class="thumbnail">
                    <%= link_to image_tag(picture.url), { :controller => 'pictures',:action => 'show' ,:name=>picture.url}  %>
                </div>
            </div>
            <% end %>
        </div>
    </div>

    <div class="container">
      <span>&nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;</span>
        <nav>
            <ul class="pagination">
                <li><a href="#">&laquo;</a></li>
                <li><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                <li><a href="#">4</a></li>
                <li><a href="#">5</a></li>
                <li><a href="#">&raquo;</a></li>
            </ul>
        </nav>

        <div class="col-sm-4">
            <%= form_for :uploadpicture, :url => upload_upload_path do |f| %>

            <%= f.file_field :url, :id => 'file-0a', :class => 'file' %>
            <%= f.hidden_field :url_cache %>
            <br>

            <!-- <%= f.submit :Upload ,:class=>'btn btn-primary'%> -->
            <%end%>

        </div>

    </div>

</div>


